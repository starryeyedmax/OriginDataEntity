{
  "version": "1.0.0.1",
  "authors": [ "Maxim Voronov" ],
  "title": "OData server via Entity Framework",
  "description": "Classes to serialize, deserialize OData JSON payloads. Supports OData v4 only. Enables construction of OData services via Entity Framework DataContext (without controller). Targets .NET Core with support for .NET 4.6",
  "buildOptions": {
    "debugType": "portable"
  },
  "packOptions": {
    "licenseUrl": "https://github.com/voronov-maxim/OdataToEntity/blob/master/LICENSE",
    "projectUrl": "https://github.com/voronov-maxim/OdataToEntity",
    "tags": [ "data", "services", "odatalib", "odata", "edmlib", "ef", "spatial", "ado.net", "framework", "entity", "protocol", "open", "core" ]
  },
  "dependencies": {
    "Microsoft.OData.Core": "7.0.0",
    "Microsoft.OData.Edm": "7.0.0",
    "Microsoft.Spatial": "7.0.0",
    "Newtonsoft.Json": "9.0.1",
    "System.ComponentModel.Annotations": "4.3.0",
    "System.ComponentModel.TypeConverter": "4.3.0"
  },
  "runtimes": {
    "win": {}
  },
  "frameworks": {
    "netstandard1.6": {
      "imports": "portable-net45+win8+wpa81",
      "dependencies": {
        "NETStandard.Library": "1.6.1"
      }
    },
    "net462": {
      "frameworkAssemblies": {
      }
    }
  }
}