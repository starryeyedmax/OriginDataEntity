{
  "version": "1.0.0.1",
  "authors": [ "Maxim Voronov" ],
  "title": "LinqToDb Core Adapter for OdataEntity",
  "description": "Classes bridge from ODataToEntity to LinqToDb Core. Abstraction layer access to DataContext Entity Linq2Db. Targets .NET Core with support for .NET 4.6",
  "buildOptions": {
    "debugType": "portable",
	"compile":"../OdataToEntity.Linq2Db/**/*.cs",
  },
  "packOptions": {
    "licenseUrl": "https://github.com/voronov-maxim/OdataToEntity/blob/master/LICENSE",
    "projectUrl": "https://github.com/voronov-maxim/OdataToEntity",
    "tags": [ "data", "services", "odatalib", "odata", "edmlib", "linq2db", "spatial", "ado.net", "framework", "entity", "protocol", "open", "core" ]
  },
  "dependencies": {
    "linq2db.Core": "1.7.5",
    "OdataToEntity": "1.0.0.*"
  },
  "runtimes": {
    "win": {}
  },
  "frameworks": {
    "netstandard1.6": {
      "imports": "portable-net45+win8+wpa81",
      "dependencies": {
        "NETStandard.Library": "1.6.1"
      }
    },
    "net462": {
      "frameworkAssemblies": {}
    }
  }
}